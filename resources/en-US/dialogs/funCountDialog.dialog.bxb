dialog (Result) {
  match: numberAndPlace(this){
    from-output: funCount
  }

    template ("There are #{value(magicNumber)} #{value(object)} in #{value(place)}") {
      speech ("<speak><lang xml:lang=\"en-GB\" voice=\"M02\">#{value(numCount)} There are #{value(magicNumber)} #{value(object)} in #{value(place)}</lang></speak>")
  }
}
